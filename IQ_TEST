#AdemHosama
n=int(input())
a=list(map(int,input().split()))
l=[]
h=[]
for i in range(len(a)):
    if a[i]%2==0:
        l.append(a[i])
        l.append(i+1)
    elif a[i]%2!=0:
        h.append(a[i])
        h.append(i+1)
if len(l)==2:
    print(l[1])
elif len(h)==2:
    print(h[1])
    
        
        
        
